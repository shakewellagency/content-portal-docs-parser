{
    "name": "shakewellagency/content-portal-docs-parser",
    "description": "This is my package content-portal-docs-parser",
    "keywords": [
        "shakewellagency",
        "content-portal-docs-parser"
    ],
    "homepage": "https://github.com/shakewellagency/content-portal-docs-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "kylyn.luterte",
            "email": "kylyn.l@shakewell.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.9",
        "phpoffice/phpword": "^1.3",
        "shakewellagency/content-portal-pdf-parser": "dev-main",
        "spatie/laravel-activitylog": "^4.9",
        "barryvdh/laravel-dompdf": "^3.0"

    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Shakewellagency\\ContentPortalDocsParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewellagency\\ContentPortalDocsParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "content-portal-pdf-parser": {
            "type": "vcs",
            "url": "https://github.com/shakewellagency/content-portal-pdf-parser"
        }
    }
}
